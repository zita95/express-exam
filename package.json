  "description": "",
  "scripts": {
    "build": "webpack",
    "dev": "concurrently \"webpack -w\" \"nodemon build/bundle.js\"",
    "start": "node build/bundle.js",
    "db:migrate": "node ./node_modules/.bin/knex migrate:up --knexfile=config/database.json",
    "db:rollback": "node ./node_modules/.bin/knex migrate:rollback --knexfile=config/database",
    "db:seed": "node ./node_modules/.bin/knex seed:run --knexfile=config/database"
  },
  "devDependencies": {
    "@types/express": "^4.17.1",
    "@types/node": "^12.7.8",
    "ts-loader": "^6.1.2",
    "typescript": "^3.6.3",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.9",
    "concurrently": "^4.1.2"
  },
  "dependencies": {
    "@types/knex": "^0.16.1",
    "@types/swagger-express-middleware": "^1.0.10",
    "express": "^4.17.1",
    "knex": "^0.20.10",
    "mysql": "^2.18.1",
    "nodemon": "^2.0.2",
    "swagger-express-middleware": "^2.0.5",
    "webpack-node-externals": "^1.7.2"
  },
  "main": "src/index.ts"
}
